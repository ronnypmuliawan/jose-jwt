<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>jose_jwt</RootNamespace>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyTitle>jose-jwt</AssemblyTitle>
    <Company>dvsekhvalnov</Company>
    <Product>jose-jwt</Product>
    <Copyright>dvsekhvalnov</Copyright>
    <VersionPrefix>1.0.0</VersionPrefix>
    <FileVersion>1.0.0.0</FileVersion>
    <VersionSuffix>ron</VersionSuffix>
    <PackageDescription>This package is a (temporary) fork of jose-jwt to allow usage on Linux</PackageDescription>
    <RepositoryUrl>https://github.com/ronnypmuliawan/jose-jwt</RepositoryUrl>
    <AssemblyName>jose-jwt-ron</AssemblyName>
    <PackageId>jose-jwt-ron</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Security.Cryptography.Cng" Version="4.7.0" />
    <PackageReference Include="System.Security.Cryptography.Algorithms" Version="4.3.1" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
